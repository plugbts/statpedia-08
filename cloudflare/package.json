{
  "name": "statpedia-cloudflare-workers",
  "version": "1.0.0",
  "description": "Cloudflare Workers for StatPedia authentication, storage, and GraphQL proxy",
  "main": "index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:auth": "wrangler deploy --name statpedia-auth auth-worker-simplified.js",
    "deploy:storage": "wrangler deploy --name statpedia-storage storage-worker.js",
    "deploy:proxy": "wrangler deploy --name statpedia-proxy proxy-worker.js",
    "tail": "wrangler tail",
    "kv:create": "wrangler kv:namespace create CACHE",
    "r2:create": "wrangler r2 bucket create statpedia-player-images"
  },
  "devDependencies": {
    "wrangler": "^3.0.0"
  },
  "dependencies": {
    
  }
}
