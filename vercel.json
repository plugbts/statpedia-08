{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "env": {
    "VITE_AUTH_ENDPOINT": "@vite_auth_endpoint",
    "NEON_DATABASE_URL": "@neon_database_url",
    "JWT_SECRET": "@jwt_secret"
  },
  "functions": {
    "src/server/api-server.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
